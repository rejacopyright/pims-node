model user {
  id            String      @id @default(uuid())
  /// 1[Regular] 2[Member] 3[Trainer] 4[Admin] 5[Owner]
  role_id       Int         @default(1)
  /// 0[Unconfirmed] 1[Active] 2[Pasive] 3[Inactive]
  status        Int         @default(1)
  username      String      @unique
  password      String?
  email         String      @unique
  phone         String
  wallet        Float?      @default(0)
  first_name    String?
  last_name     String?
  avatar        String?
  nik           String?
  nik_file      String?
  birth         DateTime?
  /// 1[Man] 2[Woman] 3[Other]
  gender        Int?
  /// 1[Yes]
  marital       Int?
  religion_id   Int?
  occupation_id Int?
  province_id   Int?
  city_id       Int?
  address       String?
  social        Json?
  created_at    DateTime    @default(now())
  updated_at    DateTime    @updatedAt
  deleted       DateTime?
  city          city?       @relation(fields: [city_id], references: [id])
  occupation    occupation? @relation(fields: [occupation_id], references: [id], onDelete: SetNull)
  province      province?   @relation(fields: [province_id], references: [id])
  religion      religion?   @relation(fields: [religion_id], references: [id])
}
